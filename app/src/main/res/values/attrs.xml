<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SwipeBackLayout">
        <attr name="edge_size" format="dimension" />
        <attr name="edge_flag">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="bottom" value="2" />
            <enum name="all" value="3" />
        </attr>
        <attr name="shadow_left" format="reference" />
        <attr name="shadow_right" format="reference" />
        <attr name="shadow_bottom" format="reference" />
    </declare-styleable>

    <attr name="SwipeBackLayoutStyle" format="reference" />

    <declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_outside_color" format="color"></attr>
    </declare-styleable>

    <declare-styleable name="ScrimInsetsView">
        <attr name="insetForeground" format="color" />
    </declare-styleable>

    <declare-styleable name="XRay">
        <attr name="LayerColor" format="reference" />
        <attr name="IndicatorForeground" format="color" />
    </declare-styleable>

    <declare-styleable name="TintImageView">
        <attr name="tintColor" format="color" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <attr name="commonBackgroundColor" format="color" />
    <attr name="writeBackgroundColor" format="color" />
    <attr name="writeEditerBackgroundColor" format="color" />
    <attr name="weibodetailBackgroundColor" format="color" />
    <attr name="weibodetailAdapterBackgroundColor" format="color" />
    <attr name="commonCardBackgroundColor" format="color" />
    <attr name="commonImageBackground" format="color" />
    <attr name="actionbarColor" format="color" />
    <attr name="accountTextColor" format="color" />
    <attr name="textColor" format="color" />
    <attr name="writeTextColor" format="color" />
    <attr name="weibodetailCommentBackground" format="color" />
    <attr name="weibodetailCommentTextColor" format="color" />
    <attr name="weibodetailNameTextColor" format="color" />
    <attr name="secondTextColor" format="color" />
    <attr name="accountBackgroundColor" format="color" />
    <attr name="dividerColor" format="color" />
</resources>
